{{#each this.results}}
<li class="gallery__item film-card">
    <img class="film-card__image" src={{#if this.poster_path}}"https://image.tmdb.org/t/p/w500{{this.poster_path}}"{{else}}"https://picsum.photos/200/300
"{{/if}} alt="{{original_title}}" data-action="{{id}}" >
    <div class="film-card__bottom">
        <div class="film-card__information">
        <h3 class="film-card__title">{{original_title}}</h3>
        <p class="film-card__genres">{{genre_ids}}</p>
         <p class="film-card__release_date">{{release_date}}</p>
        </div>
        <span class="film-card__rating">{{vote_average}}</span>
        {{!-- <span class="iddd" >{{id}}</span> --}}
     </div>
</li>
{{/each}}   